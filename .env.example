# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# API Configuration (Cloudflare Worker proxy)
NEXT_PUBLIC_API_URL=https://api.mne-select.com

# Supabase Service Role Key (Server-side only - DO NOT expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AWS Configuration (for Supabase Functions)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name
AWS_CLOUDFRONT_DOMAIN=your-cdn-domain.cloudfront.net

# Application URLs
NEXT_PUBLIC_PORTAL_URL=http://localhost:3000
NEXT_PUBLIC_GUESTS_URL=http://localhost:3001
